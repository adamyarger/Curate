

<div id="main-user-wrapper">
	<div id="user-main">
		<% posts.each(function(post){ %>
			<li class="post_show">
			      <div class="panel-heading post_media">
			        <%= post.escape("html")%>
			      </div>
			      <div class="post-body">
			        <%= post.escape('body') %>
			      </div>
			      <div class="panel-footer">

			          <div class="name_avatar u-pull-left">
			          	<span class="round-image-50 user_avatar u-pull-left">
			          		
			          	</span>
			            <span class="user_name post_name u-pull-left">
			              
			            </span>
			          </div>
			       
			      </div>
		</li>
		<% }) %>
	</div>
</div>



