<h1> sup dude </h1>

<% users.each(function(user){ %>
	<li class="user-item">
		<span class="round-image-50 user-item-pic"><img class="avatar" src="<%= user.escape("avatar") %>" /></span>
		<span class="user-item-desc">
			<a href='#/users/<%= user.escape("id") %>'>
				<%= user.escape('name') %>
			</a>
			<%= user.escape('id') %>
		</span>
	</li>
	
<% }) %>