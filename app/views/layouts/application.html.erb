<!DOCTYPE html>
<html>
<head>
  <title>Curate</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
    <body>
    	  <%= render "layouts/header" %>
        <%= render 'layouts/messages' %>

        <% if params[:action] == "root" && params[:controller] == "static_pages" %>
